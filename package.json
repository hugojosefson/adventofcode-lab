{
  "name": "adventofcode",
  "version": "1.0.0",
  "description": "Me doing the adventofcode.com exercises.",
  "scripts": {
    "start": "node -r @std/esm .",
    "test": "mocha --require @std/esm 20*/*/test.js",
    "test:watch": "mocha --watch --require @std/esm 20*/*/test.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "adventofcode",
    "javascript",
    "fp"
  ],
  "author": "Hugo Josefson <hugo@josefson.org>",
  "license": "ISC",
  "dependencies": {
    "@std/esm": "^0.17.3",
    "baconjs": "^0.7.82",
    "eval": "^0.1.1",
    "lodash": "^3.10.1",
    "md5": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^2.3.4",
    "standard": "^10.0.3"
  },
  "@std/esm": "cjs",
  "engines": {
    "node": ">=9"
  }
}
