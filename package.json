{
  "name": "adventofcode",
  "version": "1.0.0",
  "description": "Me doing the adventofcode.com exercises.",
  "scripts": {
    "start": "node -r esm .",
    "test": "mocha --require esm --require mocha-clean 20*/*/test.js",
    "test:watch": "mocha --watch --require esm --require mocha-clean 20*/*/test.js",
    "test:watch:top": "watch --color 'yarn test --colors'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "adventofcode",
    "javascript",
    "fp"
  ],
  "author": "Hugo Josefson <hugo@josefson.org>",
  "license": "ISC",
  "dependencies": {
    "baconjs": "^0.7.82",
    "esm": "^3.2.25",
    "eval": "^0.1.1",
    "lodash": "^4.17.13",
    "md5": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^6.1.4",
    "mocha-clean": "^1.0.0",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">=9"
  }
}
