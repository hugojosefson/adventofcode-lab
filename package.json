{
  "name": "adventofcode",
  "version": "1.0.0",
  "description": "Me doing the adventofcode.com exercises.",
  "scripts": {
    "start": "node -r esm .",
    "test": "run-s lint-fix mocha",
    "mocha": "mocha --require esm --require mocha-clean 20*/*/test.js",
    "mocha:watch": "mocha --watch --require esm --require mocha-clean 20*/*/test.js",
    "mocha:watch:top": "watch --color 'yarn mocha --colors'",
    "lint": "standard --verbose",
    "lint-fix": "standard --verbose --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "adventofcode",
    "javascript",
    "fp"
  ],
  "author": "Hugo Josefson <hugo@josefson.org>",
  "license": "ISC",
  "dependencies": {
    "baconjs": "^3.0.6",
    "esm": "^3.2.25",
    "eval": "^0.1.4",
    "lodash": "^4.17.14",
    "md5": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "mocha": "^6.1.4",
    "mocha-clean": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "standard": "^13.0.2"
  },
  "engines": {
    "node": ">=9"
  }
}
